    <preface>
      <p class="title">
        <shortTitle>The Indian Rifles (Repeal) Act, 2005</shortTitle>
      </p>
      <longTitle>
        <p>Act No. 49 of 2006</p>
      </longTitle>
      <p class="DateOfEnactment">
        <docDate>[17th December, 2006.]</docDate>
      </p>
